$('#tune_list').hide();
$('#tune_list').empty();

$('#tune_num').hide();
$('#tune_num').empty();

$('#tune_list').html(
    "<%= escape_javascript(render :partial => 'tunes_list_body') %>"
    );
$('#tune_list').fadeIn();
$(".progress").progressBar({
    steps           : 20,
    stepDuration    : 20,
    max            : 100,
    showText        : true,
    textFormat      : 'percentage',
    width           : 120,
    height          : 12,
    callback        : null,
    boxImage        : 'assets/progressbar.gif',
    barImage        : {
        0:  'assets/progressbg_red.gif',
        30: 'assets/progressbg_orange.gif',
        60: 'assets/progressbg_yellow.gif',
        100: 'assets/progressbg_green.gif'
    },
});

$('#tune_num').html("<%= @tunes.count %>")
$('#tune_num').fadeIn();

